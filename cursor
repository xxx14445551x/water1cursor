local q=game:GetService("Players")local w=game:GetService("RunService")local e=game:GetService("UserInputService")local r=q.LocalPlayer local t=workspace.CurrentCamera local y=math.random(99999,999999)local u=y+1 local i=y+2 local function o()local a=r.PlayerGui:FindFirstChild(tostring(y))if not a then a=Instance.new("ScreenGui")a.Name=tostring(y)a.Parent=r.PlayerGui a.IgnoreGuiInset=true a.ResetOnSpawn=false end local s=a:FindFirstChild(tostring(u))if not s then s=Instance.new("Frame")s.Name=tostring(u)s.Size=UDim2.fromOffset(260,30)s.Position=UDim2.new(1,-270,1,-10)s.BackgroundColor3=Color3.new(0,0,0)s.BackgroundTransparency=0 s.AnchorPoint=Vector2.new(1,1)s.Parent=a local d=Instance.new("UICorner")d.CornerRadius=UDim.new(0,8)d.Parent=s end local f=a:FindFirstChild(tostring(u+1))if not f then f=Instance.new("TextLabel")f.Name=tostring(u+1)f.Size=UDim2.new(1,0,1,0)f.Position=UDim2.new(0,0,0,0)f.BackgroundTransparency=1 f.Text="Nigalose discord.gg/Gc5QkQCdFA"f.Font=Enum.Font.Arcade f.TextSize=13 f.TextColor3=Color3.new(0,1,1)f.TextStrokeTransparency=0.5 f.TextStrokeColor3=Color3.new(0,0.8,1)f.Parent=s end return f end local function g()local h=r.PlayerGui:FindFirstChild(tostring(i))if not h then h=Instance.new("ScreenGui")h.Name=tostring(i)h.Parent=r.PlayerGui h.IgnoreGuiInset=true h.ResetOnSpawn=false end local j=h:FindFirstChild(tostring(i+1))if not j then j=Instance.new("TextLabel")j.Name=tostring(i+1)j.Text="Nigalose"j.Size=UDim2.fromOffset(70,15)j.BackgroundTransparency=1 j.TextColor3=Color3.new(0,1,1)j.Font=Enum.Font.Arcade j.TextSize=12 j.TextStrokeTransparency=0.5 j.TextStrokeColor3=Color3.new(0,0.8,1)j.AnchorPoint=Vector2.new(0.5,0)j.Parent=h end return j end local k=o()local l=g()local p=0 w.RenderStepped:Connect(function(z)p=(p+0.5)%360 local x=Color3.fromHSV(p/360,0.8,1)if k then k.TextColor3=x end if l then l.TextColor3=x end end)w.RenderStepped:Connect(function()if not l then return end local c if e.TouchEnabled and not e.MouseEnabled then local v=t.ViewportSize c=Vector2.new(v.X/2,v.Y/2)else c=e:GetMouseLocation()end l.Position=UDim2.fromOffset(c.X,c.Y+20)end)r.CharacterAdded:Connect(function()k=o()l=g()end)
